<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :corporate_name, 'Corporate Name'%>
    <%= f.text_field :corporate_name %>
  </div>/
  <div>
    <%= f.label :brand_name, 'Brand Name' %>
    <%= f.text_field :brand_name %>
  </div>
  <div>
    <%= f.label :registration_number, 'Registration Number' %>
    <%= f.text_field :registration_number %>
  </div>
  <div>
    <%= f.label :full_address, 'Full Address' %>
    <%= f.text_field :full_address %>
  </div>
  <div>
    <%= f.label :city, 'City' %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :state, 'State' %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.submit 'Save' %>
  </div>
<% end %>